{
    "name": "watercrawl/php",
    "description": "PHP SDK for WaterCrawl REST APIs",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "files": [
            "src/Polyfill.php"
        ],
        "psr-4": {
            "WaterCrawl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WaterCrawl\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs",
        "cs-fix": "phpcbf"
    },
    "minimum-stability": "stable"
} 